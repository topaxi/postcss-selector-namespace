{
  "name": "postcss-selector-namespace",
  "version": "1.3.0",
  "description": "A postcss plugin to prefix each rule with a specific selector",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "main": "dist/plugin.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepublish": "babel lib --out-dir dist",
    "test": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- tests/test.js",
    "lint": "eslint lib/* tests/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/topaxi/postcss-selector-namespace"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Damian Senn <damian.senn@topaxi.codes>",
  "license": "MIT",
  "keywords": [
    "postcss",
    "postcss-addon",
    "css",
    "selector",
    "prefix",
    "namespace"
  ],
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.12.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.16.1",
    "mocha": "^3.2.0",
    "postcss-scss": "^0.4.1"
  },
  "dependencies": {
    "postcss": "^5.2.15"
  }
}
