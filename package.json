{
  "name": "postcss-selector-namespace",
  "version": "2.0.0",
  "description": "A postcss plugin to prefix each rule with a specific selector",
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "6.0.0"
          }
        }
      ]
    ]
  },
  "main": "dist/plugin.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepublish": "babel lib --out-dir dist",
    "test": "nyc --require @babel/register node_modules/mocha/bin/_mocha -- tests/test.js",
    "lint": "eslint lib/**/*.js tests/**/*.js",
    "lcov": "nyc report --reporter=lcov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/topaxi/postcss-selector-namespace"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Damian Senn <damian.senn@topaxi.codes>",
  "license": "MIT",
  "keywords": [
    "postcss",
    "postcss-addon",
    "css",
    "selector",
    "prefix",
    "namespace"
  ],
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "chai-jest-snapshot": "2.0.0",
    "eslint": "5.13.0",
    "eslint-plugin-prettier": "3.0.1",
    "mocha": "5.2.0",
    "nyc": "13.3.0",
    "postcss-scss": "2.0.0"
  },
  "dependencies": {
    "postcss": "^7.0.0"
  }
}
